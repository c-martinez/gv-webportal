{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": [],
    "paths": {
      "@angular/*": [
        "node_modules/@angular/*",
        "../node_modules/@angular/*"
      ]
    }
  },
  "include":[
    "./**/*.ts",
    "../node_modules/regis-layers/**/*.ts"
  ],
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "../node_modules/regis-layers/**/*.spec.ts"
  ]
}
